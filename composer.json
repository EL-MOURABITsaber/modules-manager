{
    "name": "sabers/modules-manager",
    "description": "A Laravel package for managing modules.",
    "keywords": ["laravel", "modules", "package"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sabers\\ModulesManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sabers\\ModulesManager\\ModulesManagerServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "php artisan vendor:publish --tag=modules-manager --force"
        ]
    },
    "authors": [
        {
            "name": "EL-MOURABITsaber",
            "email": "saber.mrbet@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.10",
        "livewire/livewire": "^3.0",
        "nwidart/laravel-modules": "^10.0",
        "mhmiton/laravel-modules-livewire": "^2.1"
    },
    "require-dev": {
        "livewire/livewire": "^3.0"

    }
}
